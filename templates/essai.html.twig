<div class="element-inscription">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="sign-up">
            <div class="check-box">
              <!--<img class="img" src="img/check-box.svg" /> -->
              <p class="i-want-to-receive">
                <span class="text-wrapper">J’ai lu et j’accepte les </span>
                <span class="span">conditions générales de vente</span>
                <span class="text-wrapper">.</span>
              </p>
            </div>
            <button class="button">
              <div class="frame"><div class="div">Créer mon compte</div></div>
            </button>
            <div class="overlap-group">
              <div class="bloc-contact">
                <div class="nom-et-email">
                  <div class="div-2">
                    <div class="frame-2"><div class="label">Email</div></div>
                    <div class="text-field"></div>
                  </div>
                  <div class="div-3">
                    <div class="frame-2"><div class="label">Nom</div></div>
                    <div class="text-field"></div>
                  </div>
                </div>
                <div class="identidiant-et-tel">
                  <div class="div-2">
                    <div class="frame-2"><div class="label">Numéro de téléphone</div></div>
                    <div class="text-field"></div>
                  </div>
                  <div class="div-3">
                    <div class="frame-2"><div class="label">Identifiant</div></div>
                    <div class="text-field"></div>
                  </div>
                </div>
                <div class="bienvenue">
                  <div class="overlap-group-2">
                    <div class="pour-se-connecter">
                      <p class="already-have-an">
                        <span class="text-wrapper-2">Connectez-vous: </span>
                        <span class="text-wrapper-3">Connexion</span>
                      </p>
                    </div>
                    <div class="bienvenue-chez-KMTC">Bienvenue chez KMTC<br />Inscrivez-vous</div>
                  </div>
                </div>
                <div class="mot-de-passe">
                  <div class="text-field-2">
                    <div class="frame-2">
                      <p class="label">Créer un mot de passe</p>
                      <div class="password-hide-see">
                        <!-- <img class="icon" src="img/icon.svg" /> -->
                        <div class="text-wrapper-4">Hide</div>
                      </div>
                    </div>
                    <div class="text-field-3"></div>
                  </div>
                  <div class="frame-3">
                    <div class="frame-4">
                      <div class="bullet-point">
                        <div class="ellipse"></div>
                        <div class="one-lowercase">Utiliser minimum 8 caractères</div>
                      </div>
                      <div class="bullet-point-2">
                        <div class="ellipse"></div>
                        <div class="one-lowercase">Minimum un caractère spécial</div>
                      </div>
                    </div>
                    <div class="frame-5">
                      <div class="bullet-point">
                        <div class="ellipse"></div>
                        <div class="one-lowercase">Minimum 1 majuscule</div>
                      </div>
                      <div class="bullet-point">
                        <div class="ellipse"></div>
                        <div class="one-lowercase">Minimum un chiffre</div>
                      </div>
                    </div>
                    <div class="frame-6">
                      <div class="bullet-point">
                        <div class="ellipse"></div>
                        <div class="one-lowercase">Minimum 1 minuscule</div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="adresse">
                <div class="frame-2"><div class="label">Adresse</div></div>
                <div class="text-field"></div>
              </div>
              <div class="preference-lieu-de">
                <div class="frame-2"><div class="label">Préférence lieu de livraison</div></div>
                <div class="text-field"></div>
              </div>
            </div>
          </div>
